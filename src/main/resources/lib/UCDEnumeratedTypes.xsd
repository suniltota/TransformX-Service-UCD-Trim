	<xsd:simpleType name="AdjustmentRuleBase">
        <xsd:annotation>
            <xsd:documentation>Term: Adjustment Rule Type Definition: Specifies whether the occurrence of the adjustment is the first change or a subsequent change.</xsd:documentation>
        </xsd:annotation>
