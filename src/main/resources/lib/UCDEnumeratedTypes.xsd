	<xsd:simpleType name="LoanIdentifierBase">
		<xsd:annotation>
			<xsd:documentation>Term: Loan Identifier Type Definition: The type of identifier used for a loan.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="MISMOEnum_Base">
			<xsd:enumeration value="AgencyCase">
				<xsd:annotation>
					<xsd:documentation>An identifier assigned by a government agency (for example FHA Case Number, VA Case Number). The identifier is used by the agency to identify a loan.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="LenderLoan">
				<xsd:annotation>
					<xsd:documentation>The identifier assigned by the originating Lender to be referenced as the Loan ID/Number on all settlement documents, notes, riders, etc.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="MERS_MIN">
				<xsd:annotation>
					<xsd:documentation>Number used by MERS to identify loans. Referred to as the MIN, Mortgage Identification Number.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LoanMaturityPeriodBase">
		<xsd:annotation>
			<xsd:documentation>Term: Loan Maturity Period Type Definition: The unit of time used to define the period over which the loan matures. Used in conjunction with Loan Maturity Period Count.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="MISMOEnum_Base">
			<xsd:enumeration value="Month">
				<xsd:annotation>
					<xsd:documentation />
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LoanPurposeBase">
		<xsd:annotation>
			<xsd:documentation>Term: Loan Purpose Type Definition: Specifies the purpose for which the loan proceeds will be used.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="MISMOEnum_Base">
			<xsd:enumeration value="Purchase">
				<xsd:annotation>
					<xsd:documentation>A loan made in association with the original purchase of a piece of property.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Refinance">
				<xsd:annotation>
					<xsd:documentation>The repayment of a debt from proceeds of a new loan using the same property as security  or a mortgage secured by a property previously owned free and clear by the Borrower.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="MICompanyNameBase">
