	<xsd:include schemaLocation="UCDDataTypes.xsd"/>
	<xsd:import namespace="http://www.w3.org/1999/xlink" schemaLocation="UCDxlink.xsd"/>
	<xsd:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
	<xsd:import schemaLocation="UCDgse.xsd" namespace="http://www.datamodelextension.org" />

	<!-- complex type enum values -->
	<xsd:complexType name="AbilityToRepayExemptionReasonEnum">
		<xsd:annotation>
			<xsd:documentation>Specifies the reason that transaction is exempt from Regulation Z Ability-To-Repay requirements.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="AbilityToRepayExemptionReasonBase"/>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="AbilityToRepayMethodEnum">
		<xsd:annotation>
			<xsd:documentation>Specifies the method by which the creditor satisfied Regulation Z Ability-to-Repay requirements.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="AbilityToRepayMethodBase"/>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="ActualSignatureEnum">
        <xsd:annotation>
            <xsd:documentation>The ActualSignatureType contains the actual type of the signature applied to the signature field.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="SignatureBase">
                <xsd:attribute name="SensitiveIndicator" type="MISMOIndicator_Base">
                    <xsd:annotation>
                        <xsd:documentation>Allows producers and consumers of XML instance documents that are valid MISMO V3 documents to mark any data point to be treated as sensitive. MISMO recommends that any use of a data point labeled as "Sensitive Information" in the LDD ALWAYS have this attribute set to "true".</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="AddressEnum">
        <xsd:annotation>
            <xsd:documentation>Specifies the type of address.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="AddressBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="AddressUnitDesignatorEnum">
        <xsd:annotation>
            <xsd:documentation>An additional address designation that further defines the delivery location. Example: Apartment, Building, Condo, Suite, Room, Mail Stop, Unit, etc.  This list is based on the USPS Publication 28 on Postal Addressing Standards with the addition of Condo based on mortgage industry need.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="AddressUnitDesignatorBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="AdjustmentRuleEnum">
        <xsd:annotation>
            <xsd:documentation>Specifies whether the occurrence of the adjustment is the first change or a subsequent change.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="AdjustmentRuleBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="AmortizationEnum">
        <xsd:annotation>
            <xsd:documentation>A classification or description of a loan or a group of loans generally based on the changeability of the rate or payment over time.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="AmortizationBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="AssetEnum">
        <xsd:annotation>
            <xsd:documentation>Specifies the general names (type) of items commonly listed as financial assets of the borrower(s) in a mortgage loan transaction. Assets may be either liquid or fixed and are associated with a corresponding asset amount.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="AssetBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="AutomatedUnderwritingSystemEnum">
		<xsd:annotation>
			<xsd:documentation>The type of automated underwriting system used to evaluate the loan.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="AutomatedUnderwritingSystemBase"/>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="AutomatedUnderwritingSystemTypeOtherDescriptionEnum">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="AutomatedUnderwritingSystemTypeOtherDescriptionBase"/>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="ClosingAdjustmentItemEnum">
        <xsd:annotation>
            <xsd:documentation>The type of adjustment made during the origination process.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="ClosingAdjustmentItemBase">
				<xsd:attribute ref="gse:DisplayLabelText"/>
			</xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="ConstructionLoanEnum">
        <xsd:annotation>
            <xsd:documentation>Specifies the specific type of construction loan.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="ConstructionLoanBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="DocumentEnum">
        <xsd:annotation>
            <xsd:documentation>Identifies a specific document defined at a granular level. May be modified or further constrained by other Document data to make the document instance more specific. Further defined in the MISMO Document Classification I-Guide.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="DocumentBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="DocumentFormIssuingEntityNameEnum">
        <xsd:annotation>
            <xsd:documentation>Identifies the organization that defined the document form/template.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="DocumentFormIssuingEntityNameBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="DocumentTypeOtherDescriptionEnum">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="DocumentTypeOtherDescriptionBase"/>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="EscrowAbsenceReasonEnum">
		<xsd:annotation>
			<xsd:documentation>Specifies the reason the loan does not have an escrow account.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="EscrowAbsenceReasonBase"/>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="EscrowItemEnum">
        <xsd:annotation>
            <xsd:documentation>Specifies the type of Escrow Item.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="EscrowItemBase">
				<xsd:attribute ref="gse:DisplayLabelText"/>
			</xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="EscrowItemPaymentPaidByEnum">
        <xsd:annotation>
            <xsd:documentation>The role of the party making the escrow item payment.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="EscrowItemPaymentPaidByBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="EscrowItemPaymentTimingEnum">
        <xsd:annotation>
            <xsd:documentation>Defines the point in time during the origination process when the escrow item payment was paid.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="EscrowItemPaymentTimingBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="EventEnum">
        <xsd:annotation>
            <xsd:documentation>For a step in a process, contains the type of action or state.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="EventBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="FeeEnum">
		<xsd:annotation>
			<xsd:documentation>Specifies the general type of fees associated with a real estate finance transaction.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="FeeBase">
				<xsd:attribute ref="gse:DisplayLabelText"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="FeePaidToEnum">
        <xsd:annotation>
            <xsd:documentation>Identifies the category or type of payee to which the fee or payment will be paid.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="FeePaidToBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="FeePaidToTypeOtherDescriptionEnum">
        <xsd:annotation>
            <xsd:documentation/>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="FeePaidToTypeOtherDescriptionBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="FeePaymentPaidByEnum">
		<xsd:annotation>
			<xsd:documentation>The role of the party making the payment of fee.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="FeePaymentPaidByBase"/>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="FeePercentBasisEnum">
		<xsd:annotation>
			<xsd:documentation>Defines the amount upon which the Fee Total Percent is calculated.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="FeePercentBasisBase"/>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="FundsEnum">
        <xsd:annotation>
            <xsd:documentation>A monetary source commonly used to pay obligations in a mortgage loan transaction.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="FundsBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="IndexEnum">
        <xsd:annotation>
            <xsd:documentation>Specifies the general category of mortgage index upon which adjustments will be based.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="IndexBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="IntegratedDisclosureCashToCloseItemEnum">
        <xsd:annotation>
            <xsd:documentation>Identifies the type of cash to close item as specified in the Calculating Cash to Close section of the integrated disclosure document.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="IntegratedDisclosureCashToCloseItemBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="IntegratedDisclosureCashToCloseItemPaymentEnum">
        <xsd:annotation>
            <xsd:documentation>Identifies whether the Cash To Close Item Amount is due to or from the borrower.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="IntegratedDisclosureCashToCloseItemPaymentBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="IntegratedDisclosureSectionEnum">
        <xsd:annotation>
            <xsd:documentation>The title or description used to identify a primary section of the integrated disclosure document.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="IntegratedDisclosureSectionBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="IntegratedDisclosureSubsectionEnum">
        <xsd:annotation>
            <xsd:documentation>The title or description used to identify a secondary or subsection of the integrated disclosure document.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="IntegratedDisclosureSubsectionBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="IntegratedDisclosureSubsectionPaidByEnum">
        <xsd:annotation>
            <xsd:documentation>The role of the party making the payment of the amount associated with the Integrated Disclosure Subsection Total Amount.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="IntegratedDisclosureSubsectionPaidByBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="IntegratedDisclosureSubsectionPaymentTimingEnum">
        <xsd:annotation>
            <xsd:documentation>Defines the point in time during the origination process when the integrated disclosure subsection payment was paid.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="IntegratedDisclosureSubsectionPaymentTimingBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="LiabilityEnum">
        <xsd:annotation>
            <xsd:documentation>Specifies the general names (types) of items commonly listed as liabilities of the borrower in a mortgage loan transaction.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="LiabilityBase">
				<xsd:attribute ref="gse:DisplayLabelText"/>
			</xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="LicenseAuthorityLevelEnum">
        <xsd:annotation>
            <xsd:documentation>Level of authority of the license issuer.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="LicenseAuthorityLevelBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="LienPriorityEnum">
		<xsd:annotation>
			<xsd:documentation>Specifies the priority of the lien against the subject property.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="LienPriorityBase"/>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="LoanIdentifierEnum">
		<xsd:annotation>
			<xsd:documentation>The type of identifier used for a loan.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="LoanIdentifierBase"/>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="LoanMaturityPeriodEnum">
		<xsd:annotation>
			<xsd:documentation>The unit of time used to define the period over which the loan matures. Used in conjunction with Loan Maturity Period Count.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="LoanMaturityPeriodBase"/>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="LoanPurposeEnum">
		<xsd:annotation>
			<xsd:documentation>Specifies the purpose for which the loan proceeds will be used.</xsd:documentation>
			<xsd:documentation>Outstanding issue with MortgageModification enumeration</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="LoanPurposeBase"/>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="MICompanyNameEnum">
		<xsd:annotation>
			<xsd:documentation>To convey the private MI company short/common name from whom the private mortgage insurance coverage was obtained.</xsd:documentation>
			<xsd:documentation>  AUS  Enumerated Loan Features  MI Secondary Delivery Servicing AUS Servicing Transfer  MI Company Name Type </xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="MICompanyNameBase"/>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="MortgageEnum">
		<xsd:annotation>
			<xsd:documentation>Identifies the highest level private or public sector entity under whose guidelines the mortgage is originated.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="MortgageBase"/>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="NegativeAmortizationEnum">
		<xsd:annotation>
			<xsd:documentation>Specifies whether negative amortization is scheduled or potential on the loan. Negative amortization is unpaid interest added to the mortgage principal. This applies to loans where the principal balance increases because the payments do not cover the interest.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="NegativeAmortizationBase"/>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="ObjectEncodingEnum">
        <xsd:annotation>
            <xsd:documentation>Specifies the type of encoding used by the resource.</xsd:documentation>
            <xsd:documentation>Add references to IETF use of compression encode combimation. Add clairification about order of compression and encoding.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="ObjectEncodingBase"/>
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="PartialPaymentApplicationMethodEnum">
		<xsd:annotation>
			<xsd:documentation>The method used for applying a partial payment to the loan.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="PartialPaymentApplicationMethodBase"/>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="PartyRoleEnum">
		<xsd:annotation>
			<xsd:documentation>Identifies the role that the party plays in the transaction. Parties may be either a person or legal entity. A party may play multiple roles in a transaction.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="PartyRoleBase"/>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="PartyRoleTypeOtherDescriptionEnum">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="PartyRoleTypeOtherDescriptionBase"/>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="PaymentFrequencyEnum">
        <xsd:annotation>
            <xsd:documentation>Specifies the frequency of the mortgage payment.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="PaymentFrequencyBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="PrepaidItemEnum">
        <xsd:annotation>
            <xsd:documentation>Identification of a monthly housing expense component that must be paid in advance.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="PrepaidItemBase" >
				<xsd:attribute ref="gse:DisplayLabelText"/>
			</xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="PrepaidItemPaymentPaidByEnum">
        <xsd:annotation>
            <xsd:documentation>The role of the party making the breakout payment.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="PrepaidItemPaymentPaidByBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="PrepaidItemPaymentTimingEnum">
        <xsd:annotation>
            <xsd:documentation>Defines the point in time during the origination process when the prepaid item payment was paid.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="PrepaidItemPaymentTimingBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="PrepaidItemPerDiemCalculationMethodEnum">
        <xsd:annotation>
            <xsd:documentation>The number of days in the year used in calculating the dollar amount charged per day for the prepaid item per diem amount.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="PrepaidItemPerDiemCalculationMethodBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="ProjectedPaymentCalculationPeriodTermEnum">
        <xsd:annotation>
            <xsd:documentation>The duration of time used to define the projected payment period.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="ProjectedPaymentCalculationPeriodTermBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="ProjectedPaymentEscrowedEnum">
        <xsd:annotation>
            <xsd:documentation>Specifies if the projected payment item is to be paid from an escrow account.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="ProjectedPaymentEscrowedBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="ProjectedPaymentEstimatedTaxesInsuranceAssessmentComponentEnum">
        <xsd:annotation>
            <xsd:documentation>Specifies the type of payment component included in the calculation of the Projected Payment Estimated Taxes Insurance And Assessment Total Amount.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="ProjectedPaymentEstimatedTaxesInsuranceAssessmentComponentBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="PropertyValuationMethodEnum">
        <xsd:annotation>
            <xsd:documentation>Specifies the method by which the property value was  assessed.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="PropertyValuationMethodBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="ProrationItemEnum">
        <xsd:annotation>
            <xsd:documentation>Identification of a monthly housing expense component that must be paid in advance.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="ProrationItemBase">
				<xsd:attribute ref="gse:DisplayLabelText"/>
			</xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
	<xsd:complexType name="RealEstateAgentEnum">
        <xsd:annotation>
            <xsd:documentation>Describes the type of real estate agent.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="RealEstateAgentBase" />
        </xsd:simpleContent>
    </xsd:complexType>
	<!-- simple type enum values -->
	<xsd:simpleType name="AbilityToRepayExemptionReasonBase">
		<xsd:annotation>
			<xsd:documentation>Term: Ability To Repay Exemption Reason Type Definition: Specifies the reason that transaction is exempt from Regulation Z Ability-To-Repay requirements.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="MISMOEnum_Base">
			<xsd:enumeration value="LoanProgram">
				<xsd:annotation>
					<xsd:documentation>Indicates that transaction is exempt from Regulation Z ability-to-repay requirements because the loan program is exempt from requirements.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PropertyUsage">
				<xsd:annotation>
					<xsd:documentation>Indicates that transaction is exempt from Regulation Z ability-to-repay requirements because the occupancy type for the property is exempt from requirements.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AbilityToRepayMethodBase">
		<xsd:annotation>
			<xsd:documentation>Term: Ability To Repay Method Type Definition: Specifies the method by which the creditor satisfied Regulation Z Ability-to-Repay requirements.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="MISMOEnum_Base">
			<xsd:enumeration value="Exempt">
				<xsd:annotation>
					<xsd:documentation>A loan that is exempt from Regulation Z Ability-to-Repay requirements.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="General">
				<xsd:annotation>
					<xsd:documentation>A loan made subject to Regulation Z general Ability-to-Repay standards.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AddressBase">
        <xsd:annotation>
            <xsd:documentation>Term: Address Type Definition: Specifies the type of address.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="MISMOEnum_Base">
            <xsd:enumeration value="Mailing">
                <xsd:annotation>
                    <xsd:documentation />
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
	<xsd:simpleType name="AddressUnitDesignatorBase">
        <xsd:annotation>
            <xsd:documentation>Term: Address Unit Designator Type Definition: An additional address designation that further defines the delivery location. Example: Apartment, Building, Condo, Suite, Room, Mail Stop, Unit, etc.  This list is based on the USPS Publication 28 on Postal Addressing Standards with the addition of Condo based on mortgage industry need.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="MISMOEnum_Base">
            <xsd:enumeration value="Apartment">
                <xsd:annotation>
                    <xsd:documentation />
