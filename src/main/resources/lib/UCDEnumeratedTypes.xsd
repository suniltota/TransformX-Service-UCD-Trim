            </xsd:enumeration>
            <xsd:enumeration value="MortgageInsurance">
                <xsd:annotation>
                    <xsd:documentation />
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Other">
                <xsd:annotation>
                    <xsd:documentation />
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="ParishTax">
                <xsd:annotation>
                    <xsd:documentation />
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="PestInsurance">
                <xsd:annotation>
                    <xsd:documentation />
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="RehabilitationFunds">
                <xsd:annotation>
                    <xsd:documentation>Escrow account for rehabilitation repair or upgrade funds that are set aside as part of a Section 203(k) rehabilitation loan.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="SchoolPropertyTax">
                <xsd:annotation>
                    <xsd:documentation />
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="StatePropertyTax">
                <xsd:annotation>
                    <xsd:documentation />
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="TownPropertyTax">
                <xsd:annotation>
                    <xsd:documentation />
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="TownshipPropertyTax">
                <xsd:annotation>
                    <xsd:documentation />
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="VillagePropertyTax">
                <xsd:annotation>
                    <xsd:documentation />
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="VolcanoInsurance">
                <xsd:annotation>
                    <xsd:documentation />
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="WindstormInsurance">
                <xsd:annotation>
                    <xsd:documentation />
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
	<xsd:simpleType name="EscrowItemPaymentPaidByBase">
        <xsd:annotation>
            <xsd:documentation>Term: Escrow Item Payment Paid By Type Definition: The role of the party making the escrow item payment.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="MISMOEnum_Base">
            <xsd:enumeration value="Buyer">
                <xsd:annotation>
